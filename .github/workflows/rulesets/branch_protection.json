{
  "id": 7806771,
  "name": "branch_protection",
  "target": "branch",
  "source_type": "Repository",
  "source": "jtienhaara/utf8lex",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "exclude": [],
      "include": [
        "~DEFAULT_BRANCH"
      ]
    }
  },
  "rules": [
    {
      "type": "deletion"
    },
    {
      "type": "non_fast_forward"
    },
    {
      "type": "pull_request",
      "parameters": {
        "required_approving_review_count": 0,
        "dismiss_stale_reviews_on_push": false,
        "require_code_owner_review": false,
        "require_last_push_approval": false,
        "required_review_thread_resolution": false,
        "automatic_copilot_code_review_enabled": false,
        "allowed_merge_methods": [
          "squash"
        ]
      }
    },
    {
      "type": "required_status_checks",
      "parameters": {
        "strict_required_status_checks_policy": false,
        "do_not_enforce_on_create": false,
        "required_status_checks": [
          {
            "context": "build (linux/386)",
            "integration_id": 15368
          },
          {
            "context": "build (linux/amd64)",
            "integration_id": 15368
          },
          {
            "context": "build (linux/arm/v7)",
            "integration_id": 15368
          },
          {
            "context": "build (linux/arm64)",
            "integration_id": 15368
          },
          {
            "context": "build (linux/mips64le)",
            "integration_id": 15368
          },
          {
            "context": "build (linux/ppc64le)",
            "integration_id": 15368
          },
          {
            "context": "build (linux/s390x)",
            "integration_id": 15368
          }
        ]
      }
    }
  ],
  "bypass_actors": []
}